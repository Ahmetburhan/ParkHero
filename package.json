{
  "name": "project-ParkHero",
  "version": "1.0.0",
  "main": "server/index.js",
  "license": "MIT",
  "scripts": {
    "preinstall": "npm install --prefix server && npm install --prefix client && cd client && npm run build && cd ..",
    "start": "concurrently --kill-others-on-fail \"npm server\" \"npm client\"",
    "client": "cd client && npm start",
    "server": "cd server && nodemon index.js",
    "dev": "concurrently \"npm run client\" \"npm run server\""
  },
  "cacheDirectories": [
    "server/node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "concurrently": "^3.5.1",
    "express": "^4.16.3"
  },
  "proxy": "http://localhost:5000"
}
